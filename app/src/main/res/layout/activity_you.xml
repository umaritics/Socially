                                android:textColor="@color/black"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/startedtext5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="3dp"
                                android:layout_toRightOf="@+id/mhumpherytext1"
                                android:text="@string/started"
                                android:textColor="@color/black" />

                            <TextView
                                android:id="@+id/followingtext4"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="3dp"
                                android:layout_toRightOf="@+id/startedtext5"
                                android:text="@string/following_small"
                                android:textColor="@color/black" />

                            <TextView
                                android:id="@+id/youtext5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/mhumpherytext1"
                                android:layout_marginStart="3dp"
                                android:layout_toRightOf="@id/m_humpherypic1"
                                android:text="@string/you_small"
